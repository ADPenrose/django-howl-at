{
	"builds": [
		{
			"src": "src/vercel_app/wsgi.py",
			"use": "@vercel/python",
			"config": {
				"maxLambdaSize": "15mb",
				"runtime": "python3.9"
			}
		},
		{
			"src": "build.sh",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "src/staticfiles"
			}
		}
	],
	"routes": [
		{
			"src": "/staticfiles/(.*)",
			"dest": "/staticfiles/$1"
		},
		{
			"src": "/(.*)",
			"dest": "src/vercel_app/wsgi.py"
		}
	]
}
