{
	"builds": [
		{
			"src": "vercel_app/wsgi.py",
			"use": "@vercel/python"
		},
		{
			"src": "build.sh",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "staticfiles"
			}
		}
	],
	"routes": [
		{
			"src": "/staticfiles/(.*)",
			"dest": "/staticfiles/$1"
		},
		{
			"src": "/(.*)",
			"dest": "vercel_app/wsgi.py"
		}
	]
}
